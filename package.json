{
    "name": "bite",
    "displayName": "Bite-Language-Extension",
    "description": "Bite-Language-Extension, the language extension of the Bite programming language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.64.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "bite",
            "aliases": ["Bite", "bite"],
            "extensions": [".bite"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "bite",
            "scopeName": "source.bite",
            "path": "./syntaxes/bite.tmLanguage"
        }]
    }
}